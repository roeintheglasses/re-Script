{
  "name": "@re-script/web-api",
  "version": "1.0.0",
  "description": "Real-time web API server for re-script with Fastify",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch & nodemon dist/index.js",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@re-script/shared-types": "*",
    "@re-script/shared-utils": "*",
    "fastify": "^4.28.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/env": "^4.4.0",
    "bull": "^4.12.2",
    "redis": "^4.7.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@re-script/tsconfig": "*",
    "@types/node": "^20.14.15",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}